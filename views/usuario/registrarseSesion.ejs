 <!-- Modal registro -->
<div class="modal fade" id="registerModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
	<div class="modal-dialog">
      <div class="myform form ">
         <div class="logo mb-3">
            <div class="col-md-12 text-center">
               <h1>Regístrate</h1>
            </div>
         </div>
         <form action="/auth/registrarse" method="post" name="login">
            <div class="form-group">
               <label for="displayName">Correo </label>
               <input type="text" name="displayName"  class="form-control" id="displayName" aria-describedby="emailHelp" placeholder="Ingrese su nombre" required>
            </div>
            <div class="form-group">
               <label for="email">Correo </label>
               <input type="email" name="username"  class="form-control" id="email" aria-describedby="emailHelp" placeholder="Ingrese correo" required="true">
            </div>
            <div class="form-group">
               <label for="password">Contraseña</label>
               <input type="password" name="password" id="password"  class="form-control" aria-describedby="emailHelp" placeholder="Ingrese contraseña" required="true">
            </div>
               <div class="col-md-12 text-center ">
                  <button type="submit" class=" btn btn-block mybtn btn-primary tx-tfm">Regístrate</button>
               </div>
         </form>
      </div>
   </div>
</div>
